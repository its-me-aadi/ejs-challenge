<%-include("partials/header")%>
<h1>Contact</h1>
<form action="/compose" method="post">
    <div class="mb-3">
        <label for="title" class="form-label">Title</label>
        <input type="text" name="compose" autocomplete="off" class="form-control" id="title">
    </div>
    <div class="mb-3">
        <label for="post" class="form-label">Post</label>
        <textarea name="composePost" rows="5" cols="50" class="form-control" id="post"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" style="margin-top: 1%;" >Publish</button>
</form>
<%-include("partials/footer")%>